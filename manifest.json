{
	"name": "SiteStats",
	"version": "1.0.0",
	"description": "Chrome Extension to track your most visited sites.",
	"manifest_version": 3,
	"author": "FireStreaker2",
	"icons": {
		"16": "images/icon16.png",
		"128": "images/icon128.png"
	},
	"action": {
		"default_icon": {
			"16": "images/icon16.png",
			"128": "images/icon128.png"
		},
		"default_popup": "index.html",
		"default_title": "SiteStats"
	},
	"permissions": ["tabs", "storage"],
	"background": {
		"service_worker": "sw.js"
	}
}
